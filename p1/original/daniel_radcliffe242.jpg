		        const baseCountry = 'ar'
		        const baseApi = 'https://api.7upvselcalor.com/market/settings/' + baseCountry;

		        const mapTemp = {
		            0: 45,
		           10: 55,
		           20: 65,
		           30: 80,
		           40: 90,
		           50: 105
		        };

		        /**
		         * Obtiene parametros desde API 
		         * y retorna el porcentual con la temp. social
		         */
		        var getTemperature = function (callback) {
		          var response;
		          var rawTemp = 0;
		          var percentTemp = 0;
		          var xmlHttp = new XMLHttpRequest();
		          
		          xmlHttp.onreadystatechange = function () { 
		            if (xmlHttp.readyState == 4 && xmlHttp.status == 200) {
		              response = JSON.parse (xmlHttp.responseText);
		              rawTemp = parseInt (response.social_temperature / response.trigger_number * 100) * 10;
		              percentTemp = Math.floor ((rawTemp+1) / 10) * 10                
		                
		              return callback (
		                mapTemp[percentTemp]
		              );
		            }
		          }
		          xmlHttp.open ("GET", baseApi, true); 
		          xmlHttp.send (null);          
		        }        

		        return {
		          getTemperature: getTemperature
		        }
		      });

		      (function () {
		        var termometro = document.getElementById ('widget-7up-temperatura'); 

		        setTimeout (function () {
		          termometro.style.height = 60 + '%';
		  
		          Widget7up ().getTemperature (function (percent) {
		            termometro.style.height = percent + '%';

		            // alerta amarilla
		            if (percent <= 55) {
		              document.getElementById ('alerta-amarilla').style.opacity = 1;
		            }

		            // Alerta naranja
		            else if (percent >= 55 && percent <= 65) {
		              document.getElementById ('alerta-naranja').style.opacity = 1;
		            }

		            // Alerta roja
		            else if (percent >= 65) {
		            }

		            else {
		              document.getElementById ('alerta-amarilla').style.opacity = 1;		            	
		            }
		              document.getElementById ('alerta-roja').style.opacity = 1;
		          });

		        }, 1000);
		        
		      })();</script>  <!-- --></section>  <!--

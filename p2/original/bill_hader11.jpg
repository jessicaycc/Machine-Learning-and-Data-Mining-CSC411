                 if (_volume_hide_timeout) {
                   clearTimeout(_volume_hide_timeout);
                   _volume_hide_timeout = null;
                 }
                 var slider_data = $('#eb_volume_slider').data().sliderObject;
                 var volume = slider_data.value;
                 if (volume <= 1) {
                   slider_data.setValue(_last_volume);

            scrollEvent: function(scrollTop, $){
                var curMax, curHeight, curWidth, $curSlot;

                for(var id in this.slots){
                    curMax = this.slots[id].top;
                    $curSlot = $('#'+id);

                    if(scrollTop >= curMax){

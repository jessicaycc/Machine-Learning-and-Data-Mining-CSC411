    }

    if(typeof googletag != "undefined") {
        var lbSlot;
        
        googletag.cmd.push(function() {

            lbSlot = googletag.defineSlot('/4678/everyjoe.dmp/'+adGlobal.unit.leaderboard, adGlobal.lbSize, adGlobal.lbDiv)
